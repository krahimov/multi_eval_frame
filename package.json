{
  "name": "eval-service",
  "private": true,
  "version": "0.1.0",
  "description": "Agentsmyth Module 3 - Evaluation Framework (real-time + long-term + auto-eval)",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/bin/api.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/bin/api.js",
    "db:migrate": "tsx src/bin/migrate.ts",
    "worker:realtime": "tsx src/bin/realtimeWorker.ts",
    "job:significance": "tsx src/bin/significance.ts",
    "backtest:run": "tsx src/bin/backtest.ts",
    "job:auto-eval": "tsx src/bin/autoEval.ts",
    "job:slo-alerts": "tsx src/bin/sloAlert.ts",
    "job:anomalies": "tsx src/bin/anomalyJob.ts",
    "seed:demo": "tsx src/bin/seedDemo.ts",
    "demo:agent": "tsx src/bin/demoAgent.ts",
    "test": "npm run build --silent && node test/run-tests.mjs",
    "test:watch": "npm run test --silent",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dotenv": "^16.4.7",
    "fastify": "^4.29.1",
    "jstat": "^1.9.6",
    "pino": "^9.6.0",
    "pg": "^8.13.1",
    "prom-client": "^15.1.3",
    "uuid": "^9.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^9.0.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}


